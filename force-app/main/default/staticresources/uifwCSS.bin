.uifwcanvas{
    background-color: #f3f2f2;
    width: 100%;
    height: 100%;
}
.uifwsidebar {
  float: left;
  height: 100%;
  background-color: #16325C;    
}
.uifwsidebar ul {
  list-style-type: none;
  padding: 3px;
  text-decoration: none;
  display: block;
}
.uifwsidebar li {
  display: block;
  color: #000;
  padding: 5px;
  text-decoration: none;
  cursor: pointer;
}
.uifwMenuText {
    color: white;
    text-align: center;
}
.uifwsidebar li svg {
    fill : #C0C0C0;
}
.uifwsidebar li svg:hover {
    fill : #FFFFFF;
}
.slds-template_default{
    padding : 0 !important;
}
.uifwfloatLeft {  
  float: left; 
}
.uifwfloatright {
  float: right;
}
.ufiwalignbttom{
  position: absolute;
  bottom: 0;
  left: 0;    
}
.boxShadow{
    box-shadow: 0 2px 2px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19);
}